{
  "ActionList": {
    "create-input": {
      "Arguments": {
        "folder": "TestingWorkflow",
        "input1": "input1.txt",
        "input2": "input2.txt",
        "input3": "input3.txt",
        "input4": "input4.txt"
      },
      "InvokeNext": ["test-py-api", "test-r-api"],
      "FaaSServer": "GH",
      "Type": "Python",
      "FunctionName": "create_input"
    },
    "test-py-api": {
      "Arguments": {
        "folder": "TestingWorkflow",
        "input1": "input1.txt",
        "input2": "input2.txt",
        "input3": "input3.txt",
        "output1": "output1-py.txt",
        "output2": "output2-py.txt"
      },
      "InvokeNext": ["sync1"],
      "FaaSServer": "GH",
      "Type": "Python",
      "FunctionName": "test_py_api"
    },
    "test-r-api": {
      "Arguments": {
        "folder": "TestingWorkflow",
        "input4": "input4.txt",
        "input2": "input2.txt",
        "input3": "input3.txt",
        "output1": "output1-R.txt",
        "output2": "output2-R.txt"
      },
      "InvokeNext": ["sync1"],
      "FaaSServer": "GH",
      "Type": "R",
      "FunctionName": "test_r_api"
    },
    "sync1": {
      "Arguments": {
        "folder": "TestingWorkflow",
        "input1": "input1.txt",
        "input2": "input2.txt",
        "input3": "input3.txt",
        "input4": "input4.txt",
        "output1_py": "output1-py.txt",
        "output2_py": "output2-py.txt",
        "output1_R": "output1-R.txt",
        "output2_R": "output2-R.txt"
      },
      "InvokeNext": [
        {
          "True": ["test-run-true"],
          "False": ["test-dontrun-false"]
        }
      ],
      "FaaSServer": "GH",
      "Type": "Python",
      "FunctionName": "sync1"
    },
    "test-run-true": {
      "Arguments": {
        "folder": "TestingWorkflow",
        "output": "run_true_output.txt"
      },
      "InvokeNext": [
        {
          "False": ["test-run-false"],
          "True": ["test-dontrun-true"]
        }
      ],
      "FaaSServer": "GH",
      "Type": "Python",
      "FunctionName": "test_run_true"
    },
    "test-dontrun-false": {
      "Arguments": {},
      "InvokeNext": [],
      "FaaSServer": "GH",
      "Type": "Python",
      "FunctionName": "test_dontrun_false"
    },
    "test-run-false": {
      "Arguments": {
        "folder": "TestingWorkflow",
        "output": "run_false_output.txt"
      },
      "InvokeNext": ["test-rank(5)"],
      "FaaSServer": "GH",
      "Type": "Python",
      "FunctionName": "test_run_false"
    },
    "test-dontrun-true": {
      "Arguments": {},
      "InvokeNext": [],
      "FaaSServer": "GH",
      "Type": "Python",
      "FunctionName": "test_dontrun_true"
    },
    "test-rank": {
      "Arguments": {
        "folder": "TestingWorkflow",
        "rank_folder": "rank_files"
      },
      "InvokeNext": ["sync2"],
      "FaaSServer": "GH",
      "Type": "Python",
      "FunctionName": "test_rank"
    },
    "sync2": {
      "Arguments": {
        "folder": "TestingWorkflow",
        "rank_folder": "rank_files",
        "run_true_output": "run_true_output.txt",
        "run_false_output": "run_false_output.txt"
      },
      "InvokeNext": [],
      "FaaSServer": "GH",
      "Type": "Python",
      "FunctionName": "sync2"
    }
  },
  "ComputeServers": {
